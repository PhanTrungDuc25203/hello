<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="156.0" prefWidth="312.0" style="-fx-border-color: black;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" layoutX="3.0" layoutY="4.0" prefHeight="151.0" prefWidth="306.0" style="-fx-background-color: #eeefc4;">
         <children>
            <Label fx:id="lblTitle" alignment="CENTER" prefHeight="38.0" prefWidth="306.0" text="Title">
               <font>
                  <Font name="Maiandra GD" size="18.0" />
               </font></Label>
            <Label fx:id="lblCost" alignment="CENTER" prefHeight="37.0" prefWidth="306.0" text="Cost">
               <font>
                  <Font size="14.0" />
               </font></Label>
            <HBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="30.0" prefWidth="250.0" spacing="20.0">
               <children>
                  <Button fx:id="btnPlay" alignment="CENTER" mnemonicParsing="false" onAction="#btnPlayClicked" prefHeight="50.0" prefWidth="305.0" style="-fx-background-color: #cae0e1; -fx-border-color: black;" text="Play">
                     <font>
                        <Font size="14.0" />
                     </font></Button>
                  <Button fx:id="btnAddToCart" mnemonicParsing="false" onAction="#btnAddToCartClicked" prefHeight="50.0" prefWidth="305.0" style="-fx-background-color: #cae0e1; -fx-border-color: black;" text="Add to Cart">
                     <font>
                        <Font size="14.0" />
                     </font></Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
